<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_1109_MVVM"
        xmlns:Model="clr-namespace:_1109_MVVM.Model" xmlns:ViewModel="clr-namespace:_1109_MVVM.ViewModel" x:Class="_1109_MVVM.MainWindow"
        mc:Ignorable="d"
        Title="MainWindow" Height="248.667" Width="554.667">
    <Window.DataContext>
        <ViewModel:MainViewModel/>
    </Window.DataContext>
    <Grid>
        <Label Content="간단하게 숫자 바꾸기" HorizontalAlignment="Left" Margin="200,43,0,0" VerticalAlignment="Top"/>
        <Label Content="숫자 입력" HorizontalAlignment="Left" Margin="71,108,0,0" VerticalAlignment="Top"/>
        <Label Content="숫자 결과" HorizontalAlignment="Left" Margin="404,108,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.376,-1.756"/>
        <TextBox Text="{Binding Model.Num1, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="23" Margin="60,155,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox Text="{Binding Model.Num2, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="23" Margin="374,154,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button Content="Button" Command="{Binding btn_cmd}" HorizontalAlignment="Left" Margin="233,158,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>
