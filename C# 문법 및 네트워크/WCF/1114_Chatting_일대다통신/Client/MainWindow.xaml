<Window x:Class="Client.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Client"
        mc:Ignorable="d"
        Title="MainWindow" Height="515" Width="433">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="110*"/>
            <RowDefinition Height="131*"/>
        </Grid.RowDefinitions>
        <TextBox Name="Name" HorizontalAlignment="Left" Height="30" Margin="21,10,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="167"/>
        <Button Click="Button_Click" Content="Join" HorizontalAlignment="Left" Height="30" Margin="197,10,0,0" VerticalAlignment="Top" Width="94"/>
        <Button Click="Button_Click_1" Content="Leave" HorizontalAlignment="Left" Height="30" Margin="303,10,0,0" VerticalAlignment="Top" Width="94"/>
        <ListBox Name="ListBox" HorizontalAlignment="Center" Height="390" VerticalAlignment="Top" Width="376" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="0,50,0,0" Grid.RowSpan="2"/>
        <TextBox Name="Send" HorizontalAlignment="Left" Height="30" Margin="21,229,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="270" Grid.Row="1"/>
        <Button Click="Button_Click_2" Content="Send" HorizontalAlignment="Left" Height="30" Margin="303,228,0,0" VerticalAlignment="Top" Width="94" Grid.Row="1"/>

    </Grid>
</Window>
