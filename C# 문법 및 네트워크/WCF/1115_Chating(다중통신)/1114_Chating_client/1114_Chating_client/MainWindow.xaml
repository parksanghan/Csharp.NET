<Window x:Class="_1114_Chating_client.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_1114_Chating_client"
        mc:Ignorable="d"
        Title="MainWindow" Height="510" Width="456">
    <Grid>
        <TextBox Name="name1" HorizontalAlignment="Left" Margin="10,31,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Button Content="Join" HorizontalAlignment="Left" Margin="172,29,0,0" VerticalAlignment="Top" Width="88" Click="Button_Click"/>
        <Button Content="Leave" HorizontalAlignment="Left" Margin="311,30,0,0" VerticalAlignment="Top" Width="110" Click="Button_Click_1"/>
        <ListBox Name="listbox" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="10,76,10,45"/>
        <TextBox Name="send1" HorizontalAlignment="Left" Margin="10,438,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="322"/>
        <Button Content="Send" HorizontalAlignment="Left" Margin="347,436,0,0" VerticalAlignment="Top" Width="82" Click="Button_Click_2"/>
    </Grid>
</Window>
